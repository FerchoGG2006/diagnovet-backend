{
    "name": "diagnovet-backend",
    "version": "1.0.0",
    "description": "API para procesar reportes de ultrasonido veterinario con GCP",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "dev": "nodemon index.js",
        "test": "jest --coverage",
        "test:watch": "jest --watch"
    },
    "keywords": [
        "veterinary",
        "ultrasound",
        "gcp",
        "document-ai",
        "cloud-run"
    ],
    "author": "Fernando Baquero",
    "license": "MIT",
    "dependencies": {
        "@google-cloud/documentai": "^8.0.0",
        "@google-cloud/firestore": "^7.1.0",
        "@google-cloud/storage": "^7.7.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.5.1",
        "helmet": "^7.1.0",
        "multer": "^1.4.5-lts.1",
        "pdf-lib": "^1.17.1",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "uuid": "^9.0.0",
        "winston": "^3.19.0"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "supertest": "^6.3.4"
    },
    "jest": {
        "testEnvironment": "node",
        "coverageDirectory": "coverage",
        "collectCoverageFrom": [
            "src/**/*.js",
            "!src/**/*.test.js"
        ],
        "testMatch": [
            "**/__tests__/**/*.js",
            "**/*.test.js"
        ]
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
